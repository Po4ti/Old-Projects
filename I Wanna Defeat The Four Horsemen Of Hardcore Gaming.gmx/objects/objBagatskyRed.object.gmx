            <kind>1</kind>
            <string>if(!instance_exists(objPlayer)) {
    exit;
}

event_inherited();

if(curHP &gt; (maxHP*(4/5))) {
    event_user(0);
}

if((curHP &gt; (maxHP*(3/5))) &amp;&amp; (curHP &lt;= (maxHP*(4/5)))) {
    event_user(1);
}

if((curHP &gt; (maxHP*(2/5))) &amp;&amp; (curHP &lt;= (maxHP*(3/5)))) {
    event_user(2);
}

if((curHP &gt; (maxHP*(1/5))) &amp;&amp; (curHP &lt;= (maxHP*(2/5)))) {
    event_user(3);
}


