            <kind>1</kind>
            <string>if(waitingForFinal) {
    with(other) {
        instance_destroy();
    }
    instance_create(x,y,objBagatskyShield);
    exit;
}

if(hitTime == 0) {
    with(other) {
        instance_destroy();
    }
    audio_play_sound(sndBossHit,0,0);
    hitTime = invulTime;
    
    if(curHP &gt; 1 &amp;&amp; (curHP &lt;= (maxHP*(1/5))) &amp;&amp; finalStarted) {
        event_user(4);
    }
    
    if(curHP == 1) {
        audio_play_sound(sndTan00,0,0);
        if(!hardMode) {
            scrMakeShapes(x,y,irandom(360),5,7,6,objKnifeSpawner);
        }
        else {
            scrMakeShapes(x,y,irandom(360),6,8,6,objKnifeSpawner);
        }
        instance_create(x,y,objRainbowBulletSpawnEff);
        if(hardMode) {
            scrMakeCircle(x,y,irandom(90),4,3,objBagatskyHardmodeBullet);
        }
    }
    curHP--;
}
</string>
