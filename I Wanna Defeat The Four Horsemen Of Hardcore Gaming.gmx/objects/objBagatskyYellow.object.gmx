        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(waitingForFinal) { //Если ожидание финала
    with(other) {
        instance_destroy();
    }
    instance_create(x,y,objBagatskyShield);
    exit;
}
if(hitTime == 0) { //Если кулдауна нет
    with(other) { //Убрать пулю
        instance_destroy();
    }
    audio_play_sound(sndBossHit,0,0); //Звук
    hitTime = invulTime; //Кулдаун
    
    if((curHP &gt; (maxHP*(4/5)))) { //1 Фаза
        event_user(0);
    }
    
    if((curHP &gt; (maxHP*(3/5))) &amp;&amp; (curHP &lt;= (maxHP*(4/5)))) { //2 Фаза
        if(curHP mod 2 == 0) {
            bulCount++; //Постоянный бафф
        }
        event_user(1);
    }
    
    if((curHP &gt; (maxHP*(2/5))) &amp;&amp; (curHP &lt;= (maxHP*(3/5)))) { //3 Фаза
        event_user(2);
    }    
        
    
    if((curHP &gt; (maxHP*(1/5))) &amp;&amp; (curHP &lt;= (maxHP*(2/5)))) { //4 Фаза
        event_user(3);
        if(curHP mod 2 == 0) {
            bulCount++; //Постоянный бафф
        }
    }
    
    
    
    if((curHP &lt;= (maxHP*(1/5))) &amp;&amp; finalStarted) { //Финальная фаза
        event_user(4)
    }
    
    if(curHP == 1) { //Смерть
        scrMakeCircle(x,y,32,16,6,objCrimsonYellowBullet);
        scrMakeCircle(x,y,32,16,6,objCrimsonYellowBulletFinalClockwise);
        scrMakeCircle(x,y,32,16,6,objCrimsonYellowBulletFinalCounterClockwise);
        if(hardMode) { //Бафф для хардмода
            scrMakeCircle(x,y,irandom(90),4,3,objBagatskyHardmodeBullet);
        }
    }
    curHP--;
}
</string>
          </argument>
        </arguments>
      </action>
