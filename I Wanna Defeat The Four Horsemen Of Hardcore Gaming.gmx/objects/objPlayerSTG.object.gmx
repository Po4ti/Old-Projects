            <string>//focus
if (scrButtonCheck(global.jumpButton)){
    nowSpeed=maxSpeed/2*factor;
    slow = 1;
}
else{
    nowSpeed=maxSpeed*factor;
    slow = 0;
}
//moving
h = 0;
v = 0;
if (frozen == false)
{
    U = scrButtonCheck(global.upButton);
    D = scrButtonCheck(global.downButton);
    L = scrButtonCheck(global.leftButton);
    R = scrButtonCheck(global.rightButton);
    h = R;
    v = -U;
    if(h == 0){
        h = -L;
    }
    if(v == 0){
        v = D;
    }
    if h==0 and v!=0{
        y += nowSpeed * v;
    }
    else if h!=0 and v==0{
        x += nowSpeed * h;
    }
    else if h!=0 and v!=0{
        x += 0.7*nowSpeed*h;
        y += 0.7*nowSpeed*v;
    }

//shoot
    if (scrButtonCheck(global.shootButton)){
        if(instance_number(objBullet)&lt;32){
            a=instance_create(x,y-12,objBullet);
            a.depth=depth-1;
            a.direction=90;
            audio_play_sound(sndShoot,0,0);
        }
    }
}

//Края экрана
if (bbox_top &lt; Uborder){y = yprevious;}
if (bbox_left &lt; Lborder){x = xprevious;}
if (bbox_right &gt;= Rborder){x = xprevious;}
if (bbox_bottom &gt;= Dborder){y = yprevious;}
