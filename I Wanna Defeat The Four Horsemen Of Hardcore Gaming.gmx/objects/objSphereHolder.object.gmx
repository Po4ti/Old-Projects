rmat[0,1] = -cos(a)*sin(c)-sin(a)*cos(b)*cos(c);
rmat[0,2] = sin(a)*sin(b);

rmat[1,0] = sin(a)*cos(c)+cos(a)*cos(b)*sin(c);
rmat[1,1] = -sin(a)*sin(c)+cos(a)*cos(b)*cos(c);
rmat[1,2] = -cos(a)*sin(b);

rmat[2,0] = sin(b)*sin(c);
rmat[2,1] = sin(b)*cos(c);
rmat[2,2] = cos(b);

for( i = 0; i &lt;= rows; i++ ) {
    for( j = 0; j &lt;= cols; j++ ) {
        alpha = degtorad(a+i*360/rows);
        beta = degtorad(b+j*360/cols);
        obj[i,j].x = x + r*(rmat[0,0]*obj[i,j].xx+rmat[0,1]*obj[i,j].yy+rmat[0,2]*obj[i,j].zz);
