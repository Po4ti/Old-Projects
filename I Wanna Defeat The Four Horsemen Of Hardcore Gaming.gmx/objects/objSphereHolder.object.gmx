        obj[i,j].depth = -r*(rmat[2,0]*obj[i,j].xx+rmat[2,1]*obj[i,j].yy+rmat[2,2]*obj[i,j].zz);
        obj[i,j].image_xscale = lerp(0.5,1.5,(obj[i,j].depth+r)/(r*2));
        obj[i,j].image_yscale = obj[i,j].image_xscale;
        if( obj[i,j].depth &lt; 0 ) {
