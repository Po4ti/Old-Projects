}

if(hitTime == 0) {
    with(other) {
        instance_destroy();
    }
    audio_play_sound(sndBossHit,0,0);
    hitTime = invulTime;
    if(instance_exists(objPlayer)) {
        if(curHP &gt; (maxHP*(4/5))) {
            event_user(0);
        }
        
