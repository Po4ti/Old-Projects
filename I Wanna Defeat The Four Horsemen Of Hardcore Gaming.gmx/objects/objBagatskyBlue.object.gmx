        }
        
        if((curHP &gt; (maxHP*(1/5))) &amp;&amp; (curHP &lt;= (maxHP*(2/5)))) {
            event_user(3);
        }
        
        if(curHP &gt; 1 &amp;&amp; (curHP &lt;= (maxHP*(1/5))) &amp;&amp; finalStarted) {
            event_user(4);
        }
    }
    
    if(curHP == 1) {
